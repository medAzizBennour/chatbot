    "scripts": {
        "start": "set port=3016 && react-scripts start",
        "start:prod": "ws -d build -p 3016 -f common",
        "build": "npm run lint && react-scripts build && compress-cra",
        "postbuild": "shx rm -rf build/configuration.js.br && shx rm -rf build/configuration.js.gz",
        "test": "set CI=true && react-scripts test --coverage --testResultsProcessor ./node_modules/jest-teamcity-reporter",
        "test:dev": "react-scripts test",
        "lint": "tslint -p .",
        "fixlint": "tslint -p . --fix",
        "clean": "shx rm -rf build && shx rm -rf node_modules",
        "server": "nodemon --exec ts-node --project tsconfig.commonjs.json src/components/ChatBot/ChatBotBackend/LayoutManagerLink/server.ts",
        "whisper": "python src/components/ChatBot/ChatBotBackend/WhisperTranscribe/python_backend.py",
        "rasa": "rasa run src/components/ChatBot/ChatBotBackend/rasaBack/actions"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "nodemon": "^2.0.7",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^1.1.6",
        "@babel/core": "^7.15.5",
